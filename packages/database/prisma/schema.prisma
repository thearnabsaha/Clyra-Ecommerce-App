generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vendor {
  id          Int        @id @default(autoincrement())
  email       String     @unique
  name        String
  password    String
  products    Products[]
  time        DateTime   @default(now())
}

model Products {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Int
  userId      Int
  user        Vendor     @relation(fields: [userId], references: [id])
  time        DateTime @default(now())
}